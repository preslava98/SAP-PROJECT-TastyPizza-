<?xml version="1.0" encoding="ISO-8859-1"?>
<Configuration>
<Appenders>
<Console target="SYSTEM_OUT" name="STDOUT">
<PatternLayout pattern="%d %-5p [%t] %C{2} (%F:%L) - %m%n"/>
</Console>
</Appenders>
<Loggers>
<Logger name="hello" level="info"/>
<Root level="trace">
<AppenderRef ref="STDOUT"/>
</Root>
</Loggers>
</Configuration>